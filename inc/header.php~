<?php
$mtime = microtime(); 
$mtime = explode(" ",$mtime); 
$mtime = $mtime[1] + $mtime[0]; 
$tstart = $mtime;
 mysql_connect($INFO['base_host'], $INFO['base_user'], $INFO['base_pass']);
 mysql_select_db($INFO['base_name']);
 mysql_query("SET NAMES 'utf8'") or die("Invalid query: " . mysql_error());

?>